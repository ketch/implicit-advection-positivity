 Cell[BoxData[
  RowBox[{
   RowBox[{"firstrowOfTheInverse", "[", 
    RowBox[{"k", "+", "2"}], "]"}], "\[Equal]", 
   RowBox[{
    RowBox[{
     FractionBox["1", "\[Nu]"], 
     RowBox[{"firstrowOfTheInverse", "[", 
      RowBox[{"k", "+", "1"}], "]"}]}], "+", 
    RowBox[{"firstrowOfTheInverse", "[", "k", "]"}]}]}]],
  CellChangeTimes->{{3.6920435253016376`*^9, 3.692043656648182*^9}}],
