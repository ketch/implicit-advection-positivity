Cell[TextData[{
 "In general the Fourier series expansion on an arbitrary interval [a,b] is \n\
 ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{
     FractionBox[
      SubscriptBox["a", "0"], "2"], "+", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"n", "=", "1"}], "\[Infinity]"], 
      RowBox[{
       SubscriptBox["a", "n"], 
       RowBox[{"cos", "(", 
        FractionBox[
         RowBox[{"\[Pi]", " ", "n", " ", "x"}], "L"], ")"}]}]}], "+", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"n", "=", "1"}], "\[Infinity]"], 
      RowBox[{
       SubscriptBox["b", "n"], 
       RowBox[{"sin", "(", 
        FractionBox[
         RowBox[{"\[Pi]", " ", "n", " ", "x"}], "L"], ")"}]}]}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b6cc2bee-e0a4-4dd8-a46c-acaec10859f1"],
